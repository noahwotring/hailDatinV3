<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>



<%= form_for @user, { url: {controller: 'sessions', action: "create"} } do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br>

  <%= f.label :password %>
  <%= f.text_field :password %><br>

  <%= f.submit 'Login' %>

<% end %>
