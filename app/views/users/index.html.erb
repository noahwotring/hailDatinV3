<h1>Users#index</h1>
<p>All the users</p>



<%= @users.each do |user| %>
<ul>
  <li><%= user.username %> </li>
  <li><%= user.password %> </li>
  <% @interest = user.interest.split("").select{|char| ('abcdefghijklmnopqrstuvwxyz').include?(char.downcase) || (" ").include?(char)}.join("").split(" ") %>
<%= @interest.each do |interest|  %>
  <li> <p><%= interest %> </p> </li>
  <% end %>
</ul>


<% end  %>
